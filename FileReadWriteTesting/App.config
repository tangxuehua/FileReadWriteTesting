<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!-- ConcurrentWrite, ConcurrentRead, ParallelRead -->
    <add key="file" value="d:\data.dat"/>
    <add key="option" value="ConcurrentWrite"/>
    <add key="messageSize" value="1024"/>
    <add key="messageCount" value="5000000"/>
    <add key="concurrentThreadCount" value="4"/>
    <add key="fileReaderCount" value="4"/>
  </appSettings>
</configuration>